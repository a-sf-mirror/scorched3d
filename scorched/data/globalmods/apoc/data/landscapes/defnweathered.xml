<defn>
	<minplayers>0</minplayers>
	<maxplayers>24</maxplayers>
	<landscapewidth>256</landscapewidth>
	<landscapeheight>256</landscapeheight>
	<surround type="generate">
		<mask>data/landscapes/surround-mask.bmp</mask>
		<landhills>
			<min>32</min><max>50</max>
		</landhills>
		<landheight>
			<min>30</min><max>50</max>
		</landheight>
		<landpeakwidthx>
			<min>2</min><max>3</max>
		</landpeakwidthx>
		<landpeakwidthy>
			<min>-1</min><max>1</max>
		</landpeakwidthy>
		<landpeakheight>
			<min>1.0</min><max>1.5</max>
		</landpeakheight>
		<landsmoothing>0.04</landsmoothing>
		<levelsurround>true</levelsurround>
	</surround>
	<roof type="sky"></roof>
	<tankstart type="height">
		<startcloseness>20</startcloseness>
		<height><min>7</min><max>100</max></height>
		<startmask>data/landscapes/masks/start-half-angle1.bmp</startmask>
	</tankstart>
	<heightmap type="file">
		<file>data/landscapes/weathered.bmp</file>
		<levelsurround>true</levelsurround>
	</heightmap>
</defn>
