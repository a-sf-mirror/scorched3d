<defn>
	<minplayers>0</minplayers>
	<maxplayers>24</maxplayers>
	<landscapewidth>256</landscapewidth>
	<landscapeheight>256</landscapeheight>
	<surround type="generate">
		<mask>data/landscapes/surround-mask.bmp</mask>
		<landhills>
			<min>100</min><max>200</max>
		</landhills>
		<landheight>
			<min>40</min><max>75</max>
		</landheight>
		<landpeakwidthx>
			<min>5</min><max>10</max>
		</landpeakwidthx>
		<landpeakwidthy>
			<min>-5</min><max>5</max>
		</landpeakwidthy>
		<landpeakheight>
			<min>1.0</min><max>1.5</max>
		</landpeakheight>
		<landsmoothing>0.04</landsmoothing>
		<levelsurround>true</levelsurround>
	</surround>
	<roof type="sky"></roof>
	<tankstart type="height">
		<startcloseness>20</startcloseness>
		<height><min>10</min><max>125</max></height>
		<startmask>data/landscapes/masks/start-half-vertical.bmp</startmask>
	</tankstart>
	<heightmap type="generate">
		<mask></mask>
		<landhills>
			<min>200</min><max>400</max>
		</landhills>
		<landheight>
			<min>50</min><max>75</max>
		</landheight>
		<landpeakwidthx>
			<min>15</min><max>35</max>
		</landpeakwidthx>
		<landpeakwidthy>
			<min>-10</min><max>10</max>
		</landpeakwidthy>
		<landpeakheight>
			<min>.9</min><max>1.1</max>
		</landpeakheight>
		<landsmoothing>0.04</landsmoothing>
		<levelsurround>true</levelsurround>
	</heightmap>
</defn>
